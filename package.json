{
  "name": "react-devto",
  "version": "2.2.0",
  "description": "This is a small React Package to embed your Dev.to posts into your website.",
  "private": false,
  "main": "dist/Article.js",
  "module": "dist/Article.js",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shoaib-03/react-devto.git"
  },
  "bugs": {
    "url": "https://github.com/shoaib-03/react-devto/issues"
  },
  "homepage": "https://github.com/shoaib-03/react-devto#readme",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "publish:npm": "set NODE_ENV=production && rm -rf dist && mkdir dist && npx babel src/components/Article.js --out-dir dist --copy-files",
    "eject": "react-scripts eject"
  },
  "author": "Shoaib Sayyed",
  "keywords": [
    "react",
    "dev.to",
    "post",
    "article"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-react": "^7.9.4"
  }
}
